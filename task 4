//task 4
#include<iostream>
using namespace std;
int main()
{
    int age;
    cout<<"enter the age"<<endl;
    cin>>age;
    if (age>0)
    {
        if (age<12)
        cout<<"child";
        else if (age>12 && age<21)
        cout<<"teenager";
        else if (age>21 && age<60)
        cout<<"adult";
        else
        cout<<"senior citizen";
    }
    return 0;
}
