//task 6
#include<iostream>
using namespace std;
int main()
{
    int i,a=1,c=0,k=0;
    while(true)
    {
        for(i=1;i<=a;i++)
        {
            if (a%i==0)
            c++;
        }
        if(c==2)
        {
            cout<<a<<endl;
            k++;
        }
        a++;
        c=0;
        if(k==5)
        break;
    }
    return 0;
}
